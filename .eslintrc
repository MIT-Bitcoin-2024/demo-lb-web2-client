{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "rules": {
        // "no-unused-vars": "error"
        "no-console": "warn",
        "jsx-quotes": [
            "warn",
            "prefer-double"
        ],
        "prefer-const": "error",
        "max-len": [
            "warn",
            {
                "code": 140
            }
        ],
        // "comma-dangle": [
        //     "warn",
        //     "never"
        // ],
        "semi": [
            "warn",
            "never"
        ]
    }
}